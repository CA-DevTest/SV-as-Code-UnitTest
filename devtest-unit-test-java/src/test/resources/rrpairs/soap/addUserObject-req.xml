<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <soapenv:Body>
    <addUserObject xmlns="http://ejb3.examples.itko.com/">
      <!--userObject is optional-->
      <userObject xmlns="">
        <!--accounts is nillable and  optional, but can have an unlimited number of instances-->
        <accounts>
          <!--balance is optional-->
          <balance>100.00</balance>
          <!--id is optional-->
          <id>BtpL8LlI5kApho</id>
          <!--name is optional-->
          <name>6Cl7p3ZG</name>
          <!--type is optional-->
          <!--type supports enumerated values: CHECKING, SAVINGS, CREDIT, AUTO_LOAN, STUDENT_LOAN, MORTGAGE-->
          <type>STUDENT_LOAN</type>
        </accounts>
        <!--addresses is nillable and  optional, but can have an unlimited number of instances-->
        <addresses>
          <!--city is optional-->
          <city>Larchmont</city>
          <!--id is optional-->
          <id>3IF8aICw</id>
          <!--line1 is optional-->
          <line1>4 Forest Park Ave</line1>
          <!--line2 is optional-->
          <line2></line2>
          <!--state is optional-->
          <state>NY</state>
          <zip>10538</zip>
