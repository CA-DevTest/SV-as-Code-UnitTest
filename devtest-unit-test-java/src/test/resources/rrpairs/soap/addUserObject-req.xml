        <pwd>loothpass</pwd>
        <!--roleKey is optional-->
        <!--roleKey supports enumerated values: GUEST, REGULAR, ADMIN-->
        <roleKey>GUEST</roleKey>
        <!--ssn is optional-->
        <ssn>111111111</ssn>
      </userObject>
    </addUserObject>
  </soapenv:Body>
</soapenv:Envelope>