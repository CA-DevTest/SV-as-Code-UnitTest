        <!--email is optional-->
        <email>luther@itko.com</email>
        <!--fname is optional-->
        <fname>Luther</fname>
        <!--lname is optional-->
        <lname>Birdzell</lname>
        <!--login is optional-->
        <login>looth</login>
        <!--newFlag is optional-->
        <newFlag></newFlag>
        <!--phone is optional-->
        <phone>3035551212</phone>
